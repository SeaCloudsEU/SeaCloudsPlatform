    "bootstrap": "~3.3.5",
    "Chart.js": "~1.0.2",
    "d3": "~3.5.6",
    "angular": "~1.4.9",
    "angular-route": "~1.4.7",
    "angular-bootstrap": "~0.14.2",
    "angular-datatables": "~0.5.0",
    "angular-chart.js": "~0.8.5",
    "angular-animate": "~1.4.7",
    "angular-ui-codemirror": "~0.3.0",
    "angular-pnotify": "~0.0.6",
    "angular-loading-bar": "~0.8.0",
    "js-yaml": "~3.4.3",
    "components-font-awesome": "~4.4.0",
    "loglevel": "~1.4.0"
  },
  "overrides": {
