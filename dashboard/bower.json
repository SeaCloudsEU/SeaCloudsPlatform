{
  "name": "SeaClouds Dashboard",
  "version": "0.8.0-SNAPSHOT",
  "homepage": "https://github.com/SeaCloudsEU/SeaCloudsPlatform",
  "description": "A AngularJS based Dashboard for SeaClouds Platform",
  "keywords": [
    "SeaClouds"
  ],
  "license": "Apache-2.0",
  "ignore": [
